{
  "name": "@hopx-ai/sdk",
  "version": "0.1.21",
  "description": "Official TypeScript/JavaScript SDK for HOPX.AI Sandboxes - 100% feature coverage",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "hopx",
    "hopx-ai",
    "sandbox",
    "vm",
    "microvm",
    "cloud",
    "containers",
    "typescript",
    "sdk"
  ],
  "author": "HOPX.AI <support@hopx.ai>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hopx-ai/hopx.git",
    "directory": "javascript"
  },
  "bugs": {
    "url": "https://github.com/hopx-ai/hopx/issues"
  },
  "homepage": "https://hopx.ai",
  "dependencies": {
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "form-data": "^4.0.0",
    "glob": "^10.3.10",
    "tar": "^6.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
